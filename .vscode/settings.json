{
    "unityExplorer.preBuildCommand": "Unity Test Explorer Debug",
    "unityExplorer.testSourceFileRegex": "\\w+test.c$",
    "unityExplorer.testSourceFolder": "test",
    "unityExplorer.testCaseRegex": "TEST\\(\\w+,\\s(\\w+)\\)",
    "unityExplorer.testExecutableArgs": "-v",
    "unityExplorer.testExecutableRegex": "build/exe/$1.exe",
    "unityExplorer.unitUnderTestFileRegex": "^(?!.*test)(\\w+\\.[ch])$",
    "unityExplorer.testBuildCwdPath": "build/exe"
}